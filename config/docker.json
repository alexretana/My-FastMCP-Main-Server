{
  "host": "0.0.0.0",
  "port": 53456,
  "transport": "stdio",
  "log_level": "INFO",
  "servers": [
    {
      "name": "example-server",
      "transport": "stdio",
      "command": [
        "python",
        "-m",
        "example_mcp_server"
      ],
      "enabled": true,
      "timeout": 30,
      "namespace": "example"
    }
  ],
  "auth": {
    "enabled": false
  },
  "monitoring": {
    "enabled": true,
    "health_check_enabled": true
  },
  "log_file": "/var/log/mcp-proxy.log"
}